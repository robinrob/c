/*Program to sort a list of words using quicksort.*/

#include "sort.h"

int main(void)
{
  int N_WORDS = 4;
  char *list[] = {"a", "b", "c", "d"};
  printf("List before sort:\n");
  print_list(list, N_WORDS);
  printf("List after sort:\n");
  quicksort(list, list + N_WORDS - 1);
  print_list(list, N_WORDS);
  
  return 0;
}
