#include "sort.h"

int main(void)
{
	int a[100], i;
	srand(time(NULL));
	for (i = 0; i < 100; ++i) {
		a[i] = rand() % 100);
	}
	int *pivot_ptr;
	int pivot = find_pivot(a, a + 99, pivot_ptr, 3);
	int *p = partition(a, a + 99, *pivot_ptr);
	printf("break size: %d\n", p - a[0]);
	return 0;
}
