/*Program to sort a list of words using quicksort.*/

#include "sort.h"

int main(void)
{
  int n = 4;
  char *a[] = {"a", "b", "c", "d", "e", "f", "g"};
  printf("List before sort:\n");
  print_list(a, n);
  printf("List after sort:\n");
  quicksort(a, a + n - 1);
  print_list(a, n);
  
  return 0;
}
