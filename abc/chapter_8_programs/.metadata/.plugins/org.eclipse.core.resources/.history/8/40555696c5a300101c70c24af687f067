#include "sort.h"

void swap_string(char **p, char **q)
{
  char *c = *p;
  *p = *q;
  *q = c;
}

void print_list(char **a, int n)
{
  int i;
  for (i = 0; i < n; ++i) {
    printf("%s\n", *(a + i));
  }
}

int string_val(char *s)
{
  int i = 0, val = 0;
  while (*(s + i) != '\0') {
    val += tolower(*(s + i)) -'a';
    ++i;
  }
  return val;
}
