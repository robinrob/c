/*Program to sort a list of words using quicksort.*/

#include "sort.h"

#define MAX_LIST 50
#define MAXWORD 50

int main(void)
{
  char *a[MAX_LIST], *s;
  int i, N;

  printf("done");
  for (i = 0; i < MAX_LIST; ++i) {
    scanf("%s", s);
    printf("%s\n", s);
    if (strcmp(s, "\n") != 0) {
      strcpy(*(a + i), s);
    }
    else {
      break;
    }
  }
  N = i;

  printf("List before sort:\n");
  print_list(a, a + N - 1);
  //for (i = 0; i < N; ++i) {
   //     free(a[i]);
   // }
  //printf("\nQuicksorting ...\n");
  //quicksort(a, a + n - 1);
  //print_list(a, a + n - 1);
  
  return 0;
}
