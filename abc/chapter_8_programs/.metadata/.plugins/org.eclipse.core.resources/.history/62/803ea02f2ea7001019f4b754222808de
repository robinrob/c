#include "sort.h"

int main(void)
{
	int a[100], i, pivot, *pivot_ptr, *p;
	srand(time(NULL));
	for (i = 0; i < 100; ++i) {
		a[i] = rand() % 100;
	}
	pivot = find_pivot(a, a + 99, &pivot, 3);
	*p = partition(a, a + 99, *pivot_ptr);
	printf("break size: %d\n", p - a);
	return 0;
}
