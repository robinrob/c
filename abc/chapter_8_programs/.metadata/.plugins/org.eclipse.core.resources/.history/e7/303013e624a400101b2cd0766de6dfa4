/*Program to sort a list of words using quicksort.*/

#include "sort.h"

int main(void)
{
  char *a[] = {"hello", "i", "am", "robin"};
  int n = 4;
  printf("List before sort:\n");
  print_list(a, a + n - 1);
  printf("Quicksorting ...\n");
  quicksort(a, a + n - 1);
  printf("List after sort:\n")
  print_list(a, a + n - 1);
  
  return 0;
}
