void printA(void)
{
	printf(
			"        /^^^^\        \n"
			"       /  /\  \       \n"
			"      /  /  \  \      \n"
			"     /  /    \  \     \n"
			"    /  /______\  \    \n"
			"   /  /________\  \   \n"
			"  /  /          \  \  \n"
			" /  /            \  \ \n"
			"/__/              \__\\n"
}
