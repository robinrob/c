void printA(void)
{
	printf(
			"         /^^^^\         \n"
			"        /  /\  \        \n"
			"       /  /  \  \       \n"
			"      /  /    \  \      \n"
			"     /  /______\  \     \n"
			"    /  /________\  \    \n"
			"   /  /          \  \   \n"
			"  /  /            \  \  \n"
			" /  /              \  \ \n"
			"/__/                \__\\n"
			);
}

void printB(void)
{
	printf(
			"|^^|          \n"
			"|  |          \n"
			"|  |          \n"
			"|  |          \n"
			"|  |_________ \n"
			"|   ______   |\n"
			"|  |      |  |\n"
			"|  |      |  |\n"
			"|  |______|  |\n"
			"|____________|\n");
}
