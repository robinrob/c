/* Testing some weird arithmetic operations
 * testArithmetic.c
 *
 *  Created on: Jan 15, 2010
 *      Author: user
 */

#include <stdio.h>

int main(void)
{
	char c = 'A';
	double x = 1e33, y = 0.001;

	printf("%d %d %d\n", c == 'a', c == 'b', c != 'c');
	//0, 0, 1
	printf("%d\n", c == 'A' && c <= 'B' || 'C');
	//0
	printf("%d\n", 1 != !!c == !!!c);
	//1
	printf("%d\n", x + y > x - y);
	//1

	return 0;
}
