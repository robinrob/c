/* Checking memory address of various to see if they are stored contiguously.
 * variable_locations.c
 *
 *  Created on: Dec 9, 2010
 *      Author: user
 */

#include <stdio.h>

int main(void)
{
	char a, b, c, *p, *q, *r;

	printf("%s%p\n%s%p\n%s%p\n%s%p\n%s%p\n%s%p\n",
			"&a: ", &a,
			"&b: ", &b,
			"&c: ", &c,
			"&p: ", &p,
			"&q: ", &q,
			"&r: ", &r);

	int d = 1, e = 2, f = 3;

	printf("%s%p\n%s%p\n%s%p\n"),
			"&a = ", &a,
			"&b = ", &b,
			"&c = ", &c);

	return 0;
}
