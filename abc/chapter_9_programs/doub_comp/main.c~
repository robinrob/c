//Typedef for a structure that contains members of type double and complex.

typedef struct {
    double re;
    double im;
} complex

union doub_or_comp {
    double d;
    complex c;
}

typedef struct {
    int flag;
    doub_or_comp val;
} dc_type;


int main(void)
{
    dctype a;
    
    a.val = 3.0;
    printf("a.val: %lf\n");
    
    return 0;
}
