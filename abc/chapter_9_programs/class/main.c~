#include "class.h"

int n_fail(student class[]);

int main(void)
{
    int i;
    student class[CLASS_SIZE];
    
    srand(time(NULL));
    
    for (i = 0; i < CLASS_SIZE; ++i) {
	class[i].grade = 'A' + (rand() % 6);
    }
    
    printf("Number of students failed: %d\n", n_fail(class));
    
    return 0;
}

int n_fail(student class[])
{
    int i, cnt = 0;
    for (i = 0; i < CLASS_SIZE; ++i) {
	cnt += class[i].grade == 'F';
    }
    return cnt;
}
