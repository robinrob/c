//What gets printed?

#include <stdio.h>

typedef struct { unsigned a : 1, : 0, b : 1; } a;
typedef struct { unsigned a : 1, b : 1; } b;

int main(void)
{
	printf("sizeof(a): %.1f\n", sizeof(a));
	printf("sizeof(b): %.1f\n", sizeof(b));
	printf("ratio:     %.1f\n", (float) sizeof(a) / sizeof(b));

	return 0;
}
