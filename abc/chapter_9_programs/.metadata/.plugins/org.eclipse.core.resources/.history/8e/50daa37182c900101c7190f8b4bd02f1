//Typedef for a structure that contains members of type double and complex.

#include <stdio.h>

typedef struct {
    double re;
    double im;
} complex;

typedef union {
    double d;
    complex c;
} doub_or_comp ;

typedef struct {
    int flag;
    doub_or_comp val;
} dc_type;


int main(void)
{
    dc_type a;
    
    a.val.d = 3.0;
    a.val.c.re = 3.0;
    printf("a.val.d: %lf\n", a.val.d
    		"a.val.c.re: %lf\n", a.val.c.re
    		"a.val.c.im: %lf\n", a.val.c.im);
    
    return 0;
}
