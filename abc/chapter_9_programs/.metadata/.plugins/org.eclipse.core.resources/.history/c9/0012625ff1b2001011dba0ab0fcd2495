/* A program to test whether an input character is a
 * tab or not.
 */

#include <stdio.h>

int is_tab(char c);

int main(void)
{
	char c = getchar();
	if (is_tab(c)) {
		printf("c is a tab!\n");
	}
	else {
		printf("c is: %c\n", c);
	}
	return 0;
}

//Function to test whether or not a character is a tab.
int is_tab(char c)
{
	if (c == 2) {
		return 1;
	}
	else {
		return 0;
	}
}
