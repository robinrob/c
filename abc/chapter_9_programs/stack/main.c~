/* Program to test stack implementation. */

#include "stack.h"

int main(void)
{
    char c;
    stack stk = {0};
    printf("Enter some chars to add: ");
    while ((c = getchar()) != '\n') {
	push(c, &stk);
    }
    printf("The chars you added:\n");
    while (!(empty(&stk))) {
	printf("%c", pop(&stk));
    }
    printf("\n");
    return 0;
}
